{
  "name": "recipe-manager",
  "version": "1.0.0",
  "description": "A Node.js/Express.js/MongoDB application for managing and discovering recipes with a React frontend.",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "start:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm install && npm run build",
    "test": "cd frontend && npm test",
    "eject": "cd frontend && npm run eject"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.15.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "@mui/material": "^5.14.0",
    "@mui/icons-material": "^5.14.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
} 